            <Font name="Verdana" size="20.0" />
         </font>
      </Label>
      <Button fx:id="yesButton" layoutX="23.0" layoutY="71.0" mnemonicParsing="false" onAction="#handleYesButton" prefHeight="44.0" prefWidth="61.0" text="Yes">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button fx:id="noButton" layoutX="163.0" layoutY="71.0" mnemonicParsing="false" onAction="#handleNoButton" prefHeight="44.0" prefWidth="53.0" text="No">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
